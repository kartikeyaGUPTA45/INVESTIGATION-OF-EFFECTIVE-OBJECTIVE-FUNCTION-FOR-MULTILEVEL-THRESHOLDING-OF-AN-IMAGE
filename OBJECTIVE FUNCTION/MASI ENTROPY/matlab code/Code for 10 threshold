function obj_val=masi10(Ths,p)
% Masi Entropy Objective function for two number of thresholds
% Masi parameter r
r=-1;

Th=sort(Ths);

t1=round(Th(1));
t2=round(Th(2));
t3=round(Th(3));
t4=round(Th(4));
t5=round(Th(5));
t6=round(Th(6));
t7=round(Th(7));
t8=round(Th(8));
t9=round(Th(9));
t10=round(Th(10));

w0=sum(p(1:t1));          % class probability, w0
w1=sum(p(t1+1:t2));
w2=sum(p(t2+1:t3));
w3=sum(p(t3+1:t4));
w4=sum(p(t4+1:t5));
w5=sum(p(t5+1:t6));
w6=sum(p(t6+1:t7));
w7=sum(p(t7+1:t8));
w8=sum(p(t8+1:t9));
w9=sum(p(t9+1:t10));       % class probability, w1
w10=1-(w0+w1+w2+w3+w4);       % class probability, w1

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------
   % Calculate Entropy for class C1
    sm=0;
    if w0>0
        for i=1:t1
            if p(i)>0
                sm=sm+(p(i)/w0)*log(p(i)/w0);
            end
        end
    end
    E1=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

    % Calculate Entropy for class C2
    sm=0;
    if w1>0
        for i=t1+1:t2
            if p(i)>0
                sm=sm+(p(i)/w1)*log(p(i)/w1);
            end
        end
    end
    E2=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------
    % Calculate Entropy for class C3
    sm=0;
    if w2>0
        for i=t2+1:t3
            if p(i)>0
                sm=sm+(p(i)/w2)*log(p(i)/w2);
            end
        end
    end
    E3=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

    % Calculate Entropy for class C4
    sm=0;
    if w3>0
        for i=t3+1:t4
            if p(i)>0
                sm=sm+(p(i)/w3)*log(p(i)/w3);
            end
        end
    end
    E4=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

    % Calculate Entropy for class C5
    sm=0;
    if w4>0
        for i=t4+1:t5
            if p(i)>0
                sm=sm+(p(i)/w4)*log(p(i)/w4);
            end
        end
    end
    E5=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

    % Calculate Entropy for class C6
    sm=0;
    if w5>0
        for i=t5+1:t6
            if p(i)>0
                sm=sm+(p(i)/w5)*log(p(i)/w5);
            end
        end
    end
    E6=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

% Calculate Entropy for class C7
sm=0;
if w6>0
    for i=t6+1:t7
        if p(i)>0
            sm=sm+(p(i)/w6)*log(p(i)/w6);
        end
    end
end
E7=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

% Calculate Entropy for class C8
sm=0;
if w7>0
    for i=t7+1:t8
        if p(i)>0
            sm=sm+(p(i)/w7)*log(p(i)/w7);
        end
    end
end
E8=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

% Calculate Entropy for class C8
sm=0;
if w8>0
    for i=t8+1:t9
        if p(i)>0
            sm=sm+(p(i)/w8)*log(p(i)/w8);
        end
    end
end
E9=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

% Calculate Entropy for class C9
sm=0;
if w9>0
    for i=t9+1:t10
        if p(i)>0
            sm=sm+(p(i)/w9)*log(p(i)/w9);
        end
    end
end
E10=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

% Calculate Entropy for class C9
sm=0;
if w10>0
    for i=t10+1:256
        if p(i)>0
            sm=sm+(p(i)/w10)*log(p(i)/w10);
        end
    end
end
E11=(1/(1-r))*log(1-(1-r)*sm);

%-------------------------------------------------------------------------
%-------------------------------------------------------------------------

    % Total Masi Entropy of the Image I
    obj_val=E1+E2+E3+E4+E5+E6+E7+E8+E9+E10+E11;
end

